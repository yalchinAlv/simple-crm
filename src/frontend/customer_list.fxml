<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="700.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.181" xmlns:fx="http://javafx.com/fxml/1" fx:controller="frontend.CustomerListController">
   <children>
      <TableView fx:id="customerTable" onMouseClicked="#clickItem" prefHeight="700.0" prefWidth="700.0">
        <columns>
          <TableColumn fx:id="customerNameTab" prefWidth="75.0" style="-fx-font-family: Segoe UI Semibold;" text="Customer" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</AnchorPane>
